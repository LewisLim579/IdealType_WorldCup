<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appnight.ewhaworldcup"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/mylauncher"
        android:label="이화앱센터 이상형월드컵"
        android:screenOrientation="landscape"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape"
            android:theme="@style/MyTheme.NoTitleBar.SplashWindow" >
        </activity>
        <activity
            android:name=".SplashActivity"
            android:label="이화앱센터 이상형월드컵"
            android:screenOrientation="landscape"
            android:theme="@style/MyTheme.NoTitleBar.SplashWindow" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GenderSelectActivity"
            android:label="@string/title_activity_gender"
            android:screenOrientation="landscape"
            android:theme="@style/MyTheme.NoTitleBar.SplashWindow" >
        </activity>
        <activity
            android:name=".FinishActivity"
            android:label="@string/title_activity_finish"
            android:screenOrientation="landscape"
            android:theme="@style/MyTheme.NoTitleBar.SplashWindow" >
        </activity>
        <activity
            android:name=".GameActivity"
            android:label="@string/title_activity_male"
            android:screenOrientation="landscape"
            android:theme="@style/MyTheme.NoTitleBar.SplashWindow" >
        </activity>
        <activity
            android:name=".FeMaleActivity"
            android:label="@string/title_activity_fe_male"
            android:screenOrientation="landscape"
            android:theme="@style/MyTheme.NoTitleBar.SplashWindow" >
        </activity>
        <activity
            android:name=".RoundActivity"
            android:label="@string/title_activity_round"
            android:screenOrientation="landscape"
            android:theme="@style/MyTheme.NoTitleBar.SplashWindow" >
        </activity>
        <activity
            android:name=".AboutUsActivity"
            android:label="@string/title_activity_about_us"
            android:screenOrientation="landscape"
            android:theme="@style/MyTheme.NoTitleBar.SplashWindow" >
        </activity>
    </application>

</manifest>